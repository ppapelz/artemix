{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@artemix/server/database": ["libs/server/database/src/index.ts"],
      "@artemix/server/graphql": ["libs/server/graphql/src/index.ts"],
      "@artemix/server/models": ["libs/server/models/src/index.ts"],
      "@artemix/server/openai": ["libs/server/openai/src/index.ts"],
      "@artemix/web-auth-feature": ["libs/web/auth/feature/src/index.ts"],
      "@artemix/web-auth-feature/server": [
        "libs/web/auth/feature/src/server.ts"
      ],
      "@artemix/web-auth-ui": ["libs/web/auth/ui/src/index.ts"],
      "@artemix/web-auth-ui/server": ["libs/web/auth/ui/src/server.ts"],
      "@artemix/web-auth-util": ["libs/web/auth/util/src/index.ts"],
      "@artemix/web-auth-util/server": ["libs/web/auth/util/src/server.ts"],
      "@artemix/web-layout-feature": ["libs/web/layout/feature/src/index.ts"],
      "@artemix/web-layout-feature/server": [
        "libs/web/layout/feature/src/server.ts"
      ],
      "@artemix/web-organizations-data-access": [
        "libs/web/organizations/data-access/src/index.ts"
      ],
      "@artemix/web-organizations-data-access/server": [
        "libs/web/organizations/data-access/src/server.ts"
      ],
      "@artemix/web-organizations-feature": [
        "libs/web/organizations/feature/src/index.ts"
      ],
      "@artemix/web-organizations-feature/server": [
        "libs/web/organizations/feature/src/server.ts"
      ],
      "@artemix/web-organizations-ui": [
        "libs/web/organizations/ui/src/index.ts"
      ],
      "@artemix/web-organizations-ui/server": [
        "libs/web/organizations/ui/src/server.ts"
      ],
      "@artemix/web-shared-data-access": [
        "libs/web/shared/data-access/src/index.ts"
      ],
      "@artemix/web-shared-data-access/server": [
        "libs/web/shared/data-access/src/server.ts"
      ],
      "@artemix/web-shared-feature": ["libs/web/shared/feature/src/index.ts"],
      "@artemix/web-shared-feature/server": [
        "libs/web/shared/feature/src/server.ts"
      ],
      "@artemix/web-shared-ui": ["libs/web/shared/ui/src/index.ts"],
      "@artemix/web-shared-ui/server": ["libs/web/shared/ui/src/server.ts"],
      "@artemix/web-shared-util": ["libs/web/shared/util/src/index.ts"],
      "@artemix/web-shared-util/server": ["libs/web/shared/util/src/server.ts"],
      "@artemix/web-ui-examples-ui": ["libs/web/ui-elements/ui/src/index.ts"],
      "@artemix/web-ui-examples-ui/server": [
        "libs/web/ui-elements/ui/src/server.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
