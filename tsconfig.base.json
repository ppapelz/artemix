{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@promptus/auth-ui": ["libs/web/auth/ui/src/index.ts"],
      "@promptus/auth-ui/server": ["libs/web/auth/ui/src/server.ts"],
      "@promptus/server/database": ["libs/server/database/src/index.ts"],
      "@promptus/server/graphql": ["libs/server/graphql/src/index.ts"],
      "@promptus/server/models": ["libs/server/models/src/index.ts"],
      "@promptus/server/openai": ["libs/server/openai/src/index.ts"],
      "@promptus/ui-ai-connections": [
        "libs/web/ai-connections/ui-ai-connections/src/index.ts"
      ],
      "@promptus/ui-ai-connections/server": [
        "libs/web/ai-connections/ui-ai-connections/src/server.ts"
      ],
      "@promptus/web-shared-ui": ["libs/web/shared/ui/src/index.ts"],
      "@promptus/web-shared-ui/server": ["libs/web/shared/ui/src/server.ts"],
      "@promptus/web/auth/feature": ["libs/web/auth/feature/src/index.ts"],
      "@promptus/web/auth/feature/server": [
        "libs/web/auth/feature/src/server.ts"
      ],
      "@promptus/web/auth/ui": ["libs/web/auth/ui/src/index.ts"],
      "@promptus/web/auth/ui/server": ["libs/web/auth/ui/src/server.ts"],
      "@promptus/web/auth/util": ["libs/web/auth/util/src/index.ts"],
      "@promptus/web/auth/util/server": ["libs/web/auth/util/src/server.ts"],
      "@promptus/web/shared/data-access": [
        "libs/web/shared/data-access/src/index.ts"
      ],
      "@promptus/web/shared/data-access/server": [
        "libs/web/shared/data-access/src/server.ts"
      ],
      "@promptus/web/shared/util": ["libs/web/shared/util/src/index.ts"],
      "@promptus/web/ui-examples/ui-elements": [
        "libs/web/ui-examples/ui-elements/src/index.ts"
      ],
      "@promptus/web/ui-examples/ui-elements/server": [
        "libs/web/ui-examples/ui-elements/src/server.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
